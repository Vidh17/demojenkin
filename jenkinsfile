pipeline {
  agent any
  stages {
    stage('version') {
      steps {
        bat '"C:\\Windows\\System32\\cmd.exe" /c python --version'
      }
    }
    stage('stage2') {
      steps {
        bat '"C:\\Windows\\System32\\cmd.exe" /c python parameter.py %x_value%'
        bat '"C:\\Windows\\System32\\cmd.exe" /c python parameter.py %y_value%'
      }
    }
  }
}